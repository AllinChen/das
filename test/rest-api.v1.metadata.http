### get all environments
GET http://{{baseURL}}/api/v1/metadata/env
Accept: application/json

### get environment by id
GET http://{{baseURL}}/api/v1/metadata/env/1
Accept: application/json

### add new environment
POST http://{{baseURL}}/api/v1/metadata/env
Content-Type: application/json

{"env_name": "ctest"}

### get environment by id
POST http://{{baseURL}}/api/v1/metadata/env/6
Content-Type: application/json

{"env_name": "test123", "del_flag": 1}

### get all middleware cluster
GET http://{{baseURL}}/api/v1/metadata/middleware-cluster
Accept: application/json

### get middleware cluster by id
GET http://{{baseURL}}/api/v1/metadata/middleware-cluster/1
Accept: application/json

### add new middleware cluster
POST http://{{baseURL}}/api/v1/metadata/middleware-cluster
Content-Type: application/json

{"cluster_name": "connection_test", "env_id": 1}

### update middleware-cluster by id
POST http://{{baseURL}}/api/v1/metadata/middleware-cluster/29
Content-Type: application/json

{"cluster_name": "ccc", "del_flag": 1}

### get all middleware server
GET http://{{baseURL}}/api/v1/metadata/middleware-server
Accept: application/json

### get middleware server by id
GET http://{{baseURL}}/api/v1/metadata/middleware-server/1
Accept: application/json

### add new middleware server
POST http://{{baseURL}}/api/v1/metadata/middleware-server
Content-Type: application/json

{"cluster_id_middleware": 3, "server_name": "connection_test", "middleware_role": 1, "host_ip": "123.123.123.1", "port_num": 12}

